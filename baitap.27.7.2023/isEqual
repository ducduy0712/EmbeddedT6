#include <stdio.h>
#include <stdbool.h>
#include <stdint.h>

bool isEqual ( const char arr1[],const char arr2[])
{

    uint8_t i = 0;
    while ( arr1[i] == arr2[i])
    {
        if (arr1[i] == '\0')
        {
        return true;
        }
    i++;
}return false;
}
int main()
{
    char arr1[] = "hiio";
    char arr2[] = "hii";
    printf("test %d\n",isEqual(arr1,arr2));
    return 0;
}
